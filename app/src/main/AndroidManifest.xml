<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cikarastudio.cikaraburungkicau">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_kicau"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_kicau_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.DeskripsiActivity"></activity>
        <activity android:name=".Activity.ListBurungActivity" />
        <activity android:name=".Activity.Splashscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.MainActivity" />
    </application>

</manifest>